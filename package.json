{
  "name": "twitch_light_alerts",
  "version": "0.1.0",
  "description": "Twitch Light Alerts",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . twitch-light-alerts --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.ProductName=\"Twitch Light Alerts\"",
    "package-linux": "electron-packager . twitch-light-alerts --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "keywords": [
    "LIFX",
    "Streamlabs",
    "Tipeee",
    "Tipeee Stream",
    "Twitch",
    "Alerts",
    "Light",
    "Hue"
  ],
  "author": "Hans BÃ¶se",
  "license": "MIT",
  "dependencies": {
    "@studio/json-request": "^2.2.0",
    "bootstrap": "^4.1.3",
    "electron-store": "^1.3.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Shockwave2k/twitch-light-alerts.git"
  },
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-packager": "^12.1.2"
  }
}
